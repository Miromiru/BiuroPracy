<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping namespace="BiuroPracy.Domain" assembly="BiuroPracy.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Employee" table="Employee">
    <id name="Id" column="Id" type="Int32">
      <generator class="native" />
    </id>
    <property name="Email" type="String" column="Email" length="255" />
    <property name="Password" type="String" column="Password" length="255" />
    <property name="Name" type="String" column="Name" length="255" />
    <property name="Surname" type="String" column="Surname" length="255" />
    <property name="DateOfBirth" type="System.DateTime" column="DateOfBirth" not-null="false" />
    <many-to-one name="Contract" class="BiuroPracy.Domain.Contract" column="ContractId" fetch="select" lazy="false" cascade="none" />
    <many-to-one name="Profession" class="BiuroPracy.Domain.Profession" column="ProfessionId" fetch="select" lazy="false" cascade="none" />
  </class>
</hibernate-mapping>